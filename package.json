    "react-native-barcodescanner": "^1.0.1",
    "react-native-button": "^1.4.2",
    "react-native-camera": "git+https://github.com/lwansbrough/react-native-camera.git",
    "react-native-qrcode": "^0.1.1",
    "react-native-tabs": "^1.0.5",
    "react-native-vector-icons": "^1.2.1",
    "react-redux": "^4.4.1",
    "react-translate": "^3.0.4",
    "redux": "^3.0.3",
    "redux-actions": "^0.9.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1"
